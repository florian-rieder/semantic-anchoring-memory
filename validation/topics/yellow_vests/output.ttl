@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <http://dbpedia.org/ontology/> .
@prefix ns2: <http://example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ns2:President_Emmanuel_Macron a ns1:President ;
    rdfs:label "President Emmanuel Macron" ;
    ns2:addressOn ns2:address_on_TV .

ns2:17_November_2018 a ns1:TimeInterval ;
    rdfs:label "17 November 2018" .

ns2:address_on_TV a <Literal> ;
    rdfs:label "address on TV" ;
    ns2:ledTo ns2:evolution_of_the_movement .

ns2:evolution_of_the_movement a ns1:SocietalEvent ;
    rdfs:label "evolution of the movement" ;
    ns1:motive ns2:economic_inequality ;
    ns2:demands ns2:lower_fuel_taxes,
        ns2:minimum_wage_increase,
        ns2:wealth_tax_reintroduction ;
    ns2:motivatedBy "high cost of living",
        "rising fuel prices" ;
    ns2:receivedSupportFrom "around 3 million people worldwide" ;
    foaf:focus ns2:citizens%27_initiative_referendums .

ns2:lower_fuel_taxes a ns1:TopicalConcept ;
    rdfs:label "lower fuel taxes" .

ns2:major_riots a ns1:SocietalEvent ;
    rdfs:label "major riots" .

ns2:minimum_wage_increase a ns1:Activity ;
    rdfs:label "minimum wage increase" .

ns2:wealth_tax_reintroduction a ns1:Law ;
    rdfs:label "wealth tax reintroduction" .

ns2:citizens%27_initiative_referendums a ns1:TopicalConcept ;
    rdfs:label "citizens' initiative referendums" .

ns2:economic_inequality a ns1:TopicalConcept ;
    rdfs:label "economic inequality" .

ns2:Yellow_Vests_Protests a ns1:SocietalEvent ;
    rdfs:label "Yellow Vests Protests" ;
    ns1:knownFor ns2:Yellow_Vests_Protests ;
    ns1:namedAfter ns2:Yellow_Vests_Protests ;
    ns1:result "international attention" ;
    ns1:series ns2:Yellow_Vests_Protests ;
    ns2:advocatingFor ns2:citizens%27_initiative_referendums,
        ns2:economic_inequality ;
    ns2:beganOnDate ns2:17_November_2018 ;
    ns2:involvedIn "blocking roads and fuel depots" ;
    ns2:resultedIn "criticism of police actions" ;
    ns2:sometimesEscalatedInto ns2:major_riots .

