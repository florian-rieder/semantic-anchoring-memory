@prefix ns1: <http://dbpedia.org/ontology/> .
@prefix ns2: <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ns2:2024%20French%20farmers%27%20protests a ns1:Rebellion ;
    rdfs:label "2024 French farmers' protests" ;
    ns1:goalsInclude ns2:payment%20of%20aid%20planned%20by%20the%20state ;
    ns1:start ns2:16%20January ;
    ns2:goalsInclude ns2:end%20of%20tax%20on%20diesel ;
    ns2:ongoing "true" .

ns2:government%20buildings a ns1:ArchitecturalStructure ;
    rdfs:label "government buildings" ;
    ns1:damagedIn ns2:Occitania .

ns2:movement a ns1:Region ;
    rdfs:label "movement" ;
    ns1:start ns2:Occitania ;
    ns2:hasDamaged ns2:property .

ns2:protests a ns1:SocietalEvent ;
    rdfs:label "protests" ;
    ns1:groundsForLiquidation "scrapping of proposed reductions in government subsidies for agricultural diesel" ;
    ns1:result ns2:casualties ;
    ns2:hasPersisted "true" ;
    ns2:involvedIn ns2:blockades%20of%20autoroutes,
        "free tolls" .

ns2:16%20January a ns1:TimePeriod ;
    rdfs:label "16 January" .

ns2:blockades%20of%20autoroutes a ns1:RouteOfTransportation ;
    rdfs:label "blockades of autoroutes" ;
    ns1:routeNext "3 February 2024" .

ns2:casualties a owl:Thing ;
    rdfs:label "casualties" ;
    ns2:include "three injuries",
        "two deaths" .

ns2:end%20of%20tax%20on%20diesel a ns1:SocietalEvent ;
    rdfs:label "end of tax on diesel" .

ns2:government a ns1:GovernmentAgency ;
    rdfs:label "government" .

ns2:payment%20of%20aid%20planned%20by%20the%20state a ns1:GrossDomesticProductPerCapita ;
    rdfs:label "payment of aid planned by the state" .

ns2:property a owl:Thing ;
    rdfs:label "property" ;
    ns1:hasVariant ns2:Occitania ;
    ns2:include ns2:government .

ns2:property%20damage a owl:Thing ;
    rdfs:label "property damage" .

ns2:Occitania a ns1:PopulatedPlace ;
    rdfs:label "Occitania" ;
    ns1:location ns2:property%20damage .

